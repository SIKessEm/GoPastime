{
    "name": "sikessem/minichat",
    "description": "A small discussion platform written in PHP",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "sikessem/parsing-path": "dev-development",
        "sikessem/parsing-query": "dev-development",
        "sikessem/parsing-url": "dev-development"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "homepage": "https://sikessem.github.io",
            "name": "SKE-SIKessEm",
            "email": "sikessem@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ske\\": "lib/vendor/sikessem/framework/",
            "Ske\\Cgi\\": "lib/vendor/sikessem/cgi-app/",
            "Ske\\Cli\\": "lib/vendor/sikessem/cli-app/",
            "System\\": "lib/addons/system/"
        }
    },
    "config": {
        "vendor-dir": "lib/vendor",
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
